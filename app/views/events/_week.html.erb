<div class="table-responsive mt-3">
    <div class="col-12">
        <%= week_calendar(number_of_weeks: 1) do |date| %>
        <div><%= date.to_formatted_s(:short)[0...-4] %></div>
        <% events.each do |event| %>
        <% if event.start_time.strftime("%a, %d %b %Y") == date.strftime("%a, %d %b %Y")%>
        <div class="card rounded shadow px-3 py-2 weekly-event-list">
            <%= link_to "#{event.name[0..10]}...", event_path(event), class: "event-link" %>
        </div>
        <% end %>
        <% end %>
        <% end %>
    </div>
</div>